{
  "ReverseProxy": {
    "Routes": {
      "users-route": {
        "ClusterId": "users-cluster",
        "Match": {
          "Path": "/users/{**catch-all}"
        }
      },
      "projects-route": {
        "ClusterId": "projects-cluster",
        "Match": {
          "Path": "/projects/{**catch-all}"
        }
      },
      "tasks-route": {
        "ClusterId": "tasks-cluster",
        "Match": {
          "Path": "/tasks/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "users-cluster": {
        "Destinations": {
          "dest1": {
            "Address": "http://users-service:8080/"
          }
        }
      },
      "projects-cluster": {
        "Destinations": {
          "dest1": {
            "Address": "http://projects-service:8080/"
          }
        }
      },
      "tasks-cluster": {
        "Destinations": {
          "dest1": {
            "Address": "http://tasks-service:8080/"
          }
        }
      }
    }
  }
}
